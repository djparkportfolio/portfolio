<script src="slick/slick.min.js"></script>
<script src="script.js"></script>
<title>Audi</title>
</head>
<body>
<ul class="audi_slide">
	<li class="slide1 slides"><div class="dark"></div></li>
	<li class="slide2 slides"><div class="dark"></div></li>
	<li class="slide3 slides"><div class="dark"></div></li>
	<li class="slide4 slides"><div class="dark"></div></li>
	<li class="slide5 slides"><div class="dark"></div></li>
