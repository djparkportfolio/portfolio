	<li class="slide9 slides"><div class="dark"></div></li>
	<li class="slide10 slides"><div class="dark"></div></li>
	<li class="slide11 slides"><div class="dark"></div></li>
</ul>

<style>
	.audi_slide{height: 792px; padding: 0 10%; margin: 0; margin-top:100px;}
	.slides{height: 792px; background-size:cover; background-repeat:no-repeat; background-position:50% 0%;}
	.slides>.dark{width: 100%; height: 100%; background:rgba(0,0,0,0.5); transition:background 0.5s;}
	.slick-center .dark{background:rgba(0,0,0,0);}
	.slide1{background-image:url("audi_1.jpg");}
	.slide2{background-image:url("audi_2.jpg");}
	.slide3{background-image:url("audi_3.jpg");}
	.slide4{background-image:url("audi_4.jpg");}
	.slide5{background-image:url("audi_5.jpg");}
	.slide6{background-image:url("audi_6.jpg");}
	.slide7{background-image:url("audi_7.jpg");}
	.slide8{background-image:url("audi_8.jpg");}
	.slide9{background-image:url("audi_9.jpg");}
	.slide10{background-image:url("audi_10.jpg");}
	.slide11{background-image:url("audi_11.jpg");}
	.slick-list{padding:0 30% ! important;}
	button{border:none; background:none; }
	.slick-prev, .slick-next{position:absolute; transform:translate(0, -50%); top:50%; font-size:30px; z-index:99; color:white;}
	.slick-prev{left:12%;}
	.slick-next{right:12%;}
</style>
